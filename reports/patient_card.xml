<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>
    <template id="report_patient">
        <t t-call="web.html_container">
           <t t-foreach="docs" t-as="o"> 
            <t t-call="web.external_layout">
                <div class="page">
                    <h3 class="text-center">Test Da Antonio</h3>
                        <table class="table table-bordered mt32">
                            <thead>
                                <tr>
                                    <th class="text-center"><span>Name</span></th>
                                    <th class="text-center"><span>Date of birth</span></th>
                                    <th class="text-center"><span>Age</span></th>
                                    <th class="text-center"><span>Appointment Count</span></th>
                                    <th class="text-center"><span>Gender</span></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="text-center">
                                    <td><span t-field="o.name"/></td>
                                    <td><span t-field="o.date_of_birth"/></td>
                                    <td><span t-field="o.age"/></td>
                                    <td><span t-field="o.appointment_count"/></td>
                                    <td><span t-field="o.gender"/></td>
                                </tr>
                            </tbody>
                        </table>
                </div>
            </t>
            </t>
        </t>
    </template>
</data>
</odoo>